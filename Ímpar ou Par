import java.util.*;

class Main {
  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    int numero;
   
   
    System.out.println("Informe o numero que deseja: ");
    numero = scan.nextInt();

    for (int num = 1; num <= numero; num++) {
   if ((num%2) == 1) {
    System.out.println(num);
   }
  }}}
